<plugin id="::sight::modules::ui::dicom" class="::sight::modules::ui::dicom::Plugin"  version="@PROJECT_VERSION@" >
    <library name="module_ui_dicom" />

    
    <extension implements="::sight::service::registry::ServiceFactory">
        <type>::sight::ui::base::IEditor</type>
        <service>::sight::modules::ui::dicom::SFilterSelectionEditor</service>
        <object>::sight::data::Vector</object>
        <desc>Filter Selection Editor</desc>
    </extension>

    <extension implements="::sight::service::registry::ServiceFactory">
        <type>::sight::ui::base::IDialogEditor</type>
        <service>::sight::modules::ui::dicom::SFilterSelectorDialog</service>
        <object>::sight::data::String</object>
    </extension>

    <extension implements="::sight::service::registry::ServiceFactory">
        <type>::sight::ui::base::IAction</type>
        <service>::sight::modules::ui::dicom::SSeriesDBMerger</service>
        <object>::sight::data::Vector</object>
        <desc>Push Series to SeriesDB</desc>
    </extension>

    <extension implements="::sight::service::registry::ServiceFactory">
        <type>::sight::ui::base::IEditor</type>
        <service>::sight::modules::ui::dicom::SSliceIndexDicomEditor</service>
        <object>::sight::data::DicomSeries</object>
        <desc>Slice Index Dicom Editor</desc>
    </extension>

</plugin>
